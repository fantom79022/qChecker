<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
* @ORM\Entity
* @ORM\Table(name="summoners")
*/
class Summoners
{
/**
* @ORM\Column(type="integer")
* @ORM\Id
*/
private $id;

/**
* @ORM\Column(type="string", length=100)
*/
private $name;

/**
* @ORM\Column(type="integer")
*/
private $profileIconId;

/**
* @ORM\Column(type="integer")
*/
private $summonerLevel;

/**
* @ORM\Column(type="integer")
*/
private $revisionDate;
}